<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.example.hannah.pin_keystroke.TrainingMain"
    tools:layout_editor_absoluteY="89dp"
    tools:layout_editor_absoluteX="0dp">


<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline"
    app:layout_constraintGuide_begin="20dp"
    android:orientation="horizontal"
    tools:layout_editor_absoluteY="19dp"
    tools:layout_editor_absoluteX="0dp" />

<Button
    android:id="@+id/newUser"
    android:layout_width="200dp"
    android:layout_height="100dp"
    android:textSize="25dp"
    android:text="New User"
    tools:layout_constraintTop_creator="1"
    tools:layout_constraintRight_creator="1"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginTop="143dp"
    tools:layout_constraintLeft_creator="1"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintHorizontal_bias="0.498" />

<Button
    android:id="@+id/returningUser"
    android:layout_width="200dp"
    android:layout_height="100dp"
    android:textSize="25dp"
    android:text="Returning User"
    tools:layout_constraintTop_creator="1"
    tools:layout_constraintRight_creator="1"
    app:layout_constraintRight_toRightOf="@+id/newUser"
    android:layout_marginTop="99dp"
    app:layout_constraintTop_toBottomOf="@+id/newUser"
    tools:layout_constraintLeft_creator="1"
    app:layout_constraintLeft_toLeftOf="@+id/newUser" />

<android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline2"
    app:layout_constraintGuide_begin="20dp"
    android:orientation="horizontal"
    tools:layout_editor_absoluteY="19dp"
    tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        tools:layout_editor_absoluteX="310dp"
        tools:layout_editor_absoluteY="37dp" />

</android.support.constraint.ConstraintLayout>